{
  "name": "wingrove",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen --config graphql-codegen.ts",
    "docs-to-public": "ts-node src/lib/docs-to-public.ts",
    "generate-barrels": "barrelsby --config barrelsby.json",
    "generate-barrels:watch": "chokidar \"src/**/*.{js,jsx,ts,tsx}\" -i \"src/**/index.{js,jsx,ts,tsx}\" -c 'npm run generate-barrels'",
    "dev": "concurrently \"next dev --turbopack -p 8000\" \"npm run codegen -- --watch\" \"npm run generate-barrels:watch\" ",
    "prebuild": "npm run generate-barrels && npm run codegen",
    "build": "next build",
    "postbuild": "next-sitemap --config sitemap.config.mjs && npm run docs-to-public",
    "start": "next start -p 9000",
    "lint": "npx eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss,css}\""
  },
  "dependencies": {
    "@datocms/cda-client": "^0.2.7",
    "@gsap/react": "^2.1.2",
    "@libsql/client": "0.14",
    "@next/third-parties": "^15.5.4",
    "date-fns": "^4.1.0",
    "datocms-structured-text-utils": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^5.2.5",
    "i18n-iso-countries": "^7.14.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "libphonenumber-js": "^1.12.8",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.7.6",
    "next": "15.4.5",
    "next-sitemap": "^4.2.3",
    "node-downloader-helper": "^2.1.9",
    "normalize.css": "^8.0.1",
    "react": "19.1.0",
    "react-datocms": "^7.2.2",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-inlinesvg": "^4",
    "react-intersection-observer": "^9.16.0",
    "react-player": "^2.16.0",
    "sass": "^1.89.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/near-operation-file-preset": "^3.0.1",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@parcel/watcher": "^2.5.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/smoothscroll-polyfill": "^0.3.4",
    "barrelsby": "^2.8.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fs-extra": "^11.3.0",
    "globals": "^16.2.0",
    "globby": "^14.1.0",
    "graphql": "^16.11.0",
    "jscodeshift": "^17.3.0",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.33.1"
  }
}
